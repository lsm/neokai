{
	"name": "@liuboer/daemon",
	"version": "0.1.0",
	"type": "module",
	"main": "main.ts",
	"exports": {
		".": "./main.ts",
		"./app": "./src/app.ts",
		"./config": "./src/config.ts"
	},
	"scripts": {
		"dev": "bun run --watch main.ts",
		"start": "NODE_ENV=production bun run main.ts",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage",
		"test:coverage:lcov": "bun test --coverage --coverage-reporter=lcov"
	},
	"dependencies": {
		"@anthropic-ai/claude-agent-sdk": "0.1.60",
		"@elysiajs/cors": "1.4.0",
		"@liuboer/shared": "workspace:*",
		"dotenv": "17.2.3",
		"elysia": "1.4.18"
	},
	"devDependencies": {
		"@types/bun": "latest"
	}
}
